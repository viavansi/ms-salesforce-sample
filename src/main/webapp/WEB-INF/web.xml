<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Viafirma Documents - Sales Force Test</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>SampleResponseServlet</display-name>
    <servlet-name>SampleResponseServlet</servlet-name>
    <servlet-class>com.viafirma.documents.sample.SampleResponseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SampleResponseServlet</servlet-name>
    <url-pattern>/response</url-pattern>
  </servlet-mapping>
</web-app>